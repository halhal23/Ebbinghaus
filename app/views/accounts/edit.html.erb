<div class="form-wrapper">
  <%= render partial: "shared/error", locals: { obj: @user } %>
  <%= form_for @user, as: "account", url: :account do |f| %>
    <div class="form-group">
    <%= f.text_field :name, class: "form-control text-input", placeholder: "Name" %>
    </div>
    <div class="form-group">
    <%= f.email_field :email, class: "form-control text-input", placeholder: "Email" %>
    </div>
    
    <%= f.submit "Submit", class: "btn btn-outline-primary" %>
    <% end %>
</div>